{
  "name": "nuxt-app",
  "version": "1.2.8",
  "private": false,
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/icon": "1.11.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/test-utils": "3.17.2",
    "@pinia/nuxt": "0.10.1",
    "eslint": "^9.0.0",
    "nuxt": "^3.16.2",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify-nuxt-module": "0.18.5"
  },
  "devDependencies": {
    "@nuxtjs/i18n": "^9.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": true,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
      {
        "files": "CHANGELOG.md",
        "options": {
          "tabWidth": 2,
          "parser": "markdown"
        }
      },
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2,
          "parser": "json-stringify"
        }
      }
    ]
  }
}