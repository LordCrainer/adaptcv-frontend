{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "@domains/*": [
        "./domains/*"
      ],
    }
  },
  "include": ["types/**/*.d.ts", "nuxt.config.ts", "app.config.ts", "app/**/*.vue", "app/**/*.ts", "app/**/*.js"],
}
